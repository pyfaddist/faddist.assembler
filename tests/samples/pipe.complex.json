{
  "alias": [
    {
      "name": "db_reader",
      "__type__": "faddist.iterators.DatabaseIterator"
    },
    {
      "name": "map",
      "__type__": "rx.operators.map"
    }
  ],
  "variables": [
    {
      "name": "bucket",
      "__type__": "list"
    },
    {
      "name": "engine",
      "__type__": "sqlalchemy.create_engine",
      "arguments": "sqlite:///test.db"
    }
  ],
  "iterator": {
    "__alias__": "db_reader",
    "arguments": [
      "$var:engine",
      "SELECT * FROM test"
    ]
  },
  "pipe": [
    {}
  ],
  "observer": "$lambda x: bucket.append(x)"
}
